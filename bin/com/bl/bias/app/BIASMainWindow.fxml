            <Insets top="10.0" />
         </GridPane.margin>
        </ImageView>
        <Label alignment="BOTTOM_LEFT" prefHeight="50.0" prefWidth="452.0" text="Internal Analysis System" textAlignment="LEFT" GridPane.rowIndex="1">
			<font>
				<Font name="Century Gothic" size="36.0" />
			</font>
			<GridPane.margin>
				<Insets left="300.0" top="10.0" />
			</GridPane.margin>
		</Label>
