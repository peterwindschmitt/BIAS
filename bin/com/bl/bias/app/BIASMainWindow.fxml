                  </Menu>
                  <Menu mnemonicParsing="false" text="Compliance">
                     <items>
                        	<MenuItem fx:id="menuItemUscgBridgeCompliance" mnemonicParsing="true" onAction="#handleMenuItemUscgBridgeCompliance" text="USCG Bridge Compliance" visible="false" />
